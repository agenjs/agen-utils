{
  "name": "@agen/utils",
  "version": "0.20.1",
  "description": "Async Generator utilities",
  "keywords": [
    "async",
    "generators"
  ],
  "homepage": "https://github.com/agenjs/agen-utils/",
  "license": "BSD-3-Clause",
  "author": {
    "name": "Mikhail Kotelnikov"
  },
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "module": "./dist/index.js",
  "main": "./dist/index.js",
  "jsdelivr": "./dist/index.js",
  "unpkg": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agenjs/agen-utils.git"
  },
  "scripts": {
    "build": "rm -rf dist && tsup",
    "zip": "zip -j dist/agen-utils.zip -- LICENSE README.md dist/index.js dist/index.d.ts",
    "watch": "tsup --watch sandbox.ts",
    "clean": "rm -rf dist",
    "lint": "eslint \"**/*.ts\"",
    "test": "vitest",
    "prepublishOnly": "rm -rf dist && yarn test --run && yarn build",
    "postpublish": "yarn zip"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@statewalker/eslint-config": "*",
    "@statewalker/typescript-config": "*",
    "@types/node": "^20.11.16",
    "eslint": "^8.56.0",
    "expect.js": "^0.3.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  }
}
