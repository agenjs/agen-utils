{
  "name": "@agen/utils",
  "version": "0.11.0",
  "description": "Async Generator utilities",
  "keywords": [
    "async",
    "generators"
  ],
  "homepage": "https://github.com/agenjs/agen-utils/",
  "license": "BSD-3-Clause",
  "author": {
    "name": "Mikhail Kotelnikov"
  },
  "type": "module",
  "files": [
    "dist/**/package.json",
    "dist/**/*.js",
    "src/**/*.js"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "jsdelivr": "dist/index.js",
  "unpkg": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/agenjs/agen-utils.git"
  },
  "scripts": {
    "build": "rm -rf dist && tsup && cp ./src/index.d.ts ./dist/",
    "zip" : "zip -j dist/agen-utils.zip -- LICENSE README.md dist/index.js dist/index.d.ts",
    "watch": "tsup --watch sandbox.ts",
    "clean": "rm -rf dist",
    "lint": "eslint \"**/*.ts\"",
    "test": "vitest",
    "prepublishOnly": "rm -rf dist && yarn test && yarn build",
    "postpublish": "yarn zip"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "eslint": "^8.56.0",
    "expect.js": "^0.3.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  }
}
